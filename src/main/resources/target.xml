        <URL>url_to_loc</URL>
        <transformation1>xtransformation1.class/function</transformation1>
        <transformation2>xtransformation2.class/function</transformation2>
        <transformation3>xtransformation3.class/function</transformation3>
        <transformation4>xtransformation4.class/function</transformation4>
    </transformation>

    <transformation>
