        <data_column>Data</data_column>
    </transformationSource>

    <transformation>
        <type>json</type>
        <URL>url_to_loc</URL>
        <transformation>xtransformation1.class/function</transformation>
        <transformation>xtransformation2.class/function</transformation>
        <transformation>xtransformation3.class/function</transformation>
        <transformation>xtransformation4.class/function</transformation>
    </transformation>

    <transformation>
        <type>csv</type>
        <URL>url_to_loc</URL>
        <transformation>ytransformation1.class/function</transformation>
        <transformation>ytransformation2.class/function</transformation>
        <transformation>ytransformation3.class/function</transformation>
        <transformation>ytransformation4.class/function</transformation>
    </transformation>

    <transformationTarget>
        <table>foreign_exchange</table>
        <column>transformation1-res</column>
        <column>transformation2-res</column>
        <column>transformation3-res</column>
        <column>transformation4-res</column>
